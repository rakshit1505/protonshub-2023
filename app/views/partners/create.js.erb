$('.inline-error').html('')
<% if @partner.errors.present? %>
  <% @partner.errors.each do |err| %>
    var err_class =  ".partner-error-<%= err.attribute.to_s %>"
    if($(err_class).text() == ''){
      $(err_class).text("<%= raw err.full_message.downcase %>")
      $(err_class).addClass('text-danger')
    }
  <% end %>
<% else %>
  window.location.href = "<%= ENV['HOST_URL'] %>/thank_you"
<% end %>
