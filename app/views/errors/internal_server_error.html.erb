<div class="ph-main-wrapper">
  <!-- Header start -->
  <%= render partial: 'home/header' %>
  <!-- Header end -->

  <!-- Middle Content Start -->
  <section class="ph-internalservererror-sec">
    <div class="container">
      <div class="row align-items-center">
        <div class="ph-internalservererror text-center">
          <%= image_tag "422.png" %>
          <div class="ph-four-page">
            <h3>Oops! internal server error.</h3>
            <p>We're sorry, but something went wrong.</p>
            <div class="ph-banner-btn text-center mb-4 mt-5">
              <%= link_to root_url, class: 'ph-btn' do %>
                Go Back to Home<i class="fa-solid fa-arrow-right ms-2"></i>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- Middle Content End -->

  <!-- footer Sec start -->
  <%= render partial: 'home/footer' %>
  <!-- footer sec end -->
</div>

<!-- Modal -->
<%= render partial: 'home/hire_developer' %>