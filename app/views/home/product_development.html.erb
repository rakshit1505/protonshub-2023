      <div class="row">
        <div class="ph-process-sec">
          <div class="ph-process-in">
            <div class="ph-process-inner">
              <div class="ph-count-no">
                <h2>1</h2>
              </div>
              <div class="ph-outer-in">
                <div class="ph-picon-box">
                  <%= image_tag "analysis.svg" %>
