<div class="modal ph-modal-blur fade" id="ph-hire-developer" tabindex="-1">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <button type="button" class="btn-close ph-close-btn" data-bs-dismiss="modal"
      aria-label="Close"></button>
      <div class="ph-contact-field ph-hire-form">
        <div class="ph-modal-heading">
          <h4>Hire Developer</h4>
          <p>Create Stunning App & websites for your online business</p>
        </div>
        <form method="post" class="ph-protons-form" action="">
          <div class="row">
            <div class="col-md-12">
              <input type="text" name="name" placeholder="Full Name*" required="">
            </div>
            <div class="col-md-12">
              <input type="email" name="email" placeholder="Email" required="">
            </div>
            <div class="col-md-12">
              <input type="number" name="number" placeholder="Phone*" required="">
            </div>
            <div class="col-md-12">
              <select class="form-select" required="">
                <option selected>Select Technology*</option>
                <option value="1">One</option>
                <option value="2">Two</option>
                <option value="3">Three</option>
              </select>
            </div>
            <div class="col-md-12">
              <div class="ph-contact-msg mt-0">
                <textarea name="message" placeholder="Brief About The Project" required=""></textarea>
              </div>
            </div>
            <div class="col-md-12 text-center d-block">
              <a href="javascript:;" class="ph-btn ph-contact-btn">Submit</a>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>