$('.inline-error').html('')
<% if @project.errors.present? %>
  <% @project.errors.each do |err| %>
    var err_class =  ".project-error-<%= err.attribute.to_s %>"
    $(err_class).text("<%= raw err.full_message.downcase %>")
    $(err_class).addClass('text-danger')
  <% end %>
<% else %>
  window.location.href = "<%= ENV['HOST_URL'] %>/thank_you"
<% end %>
